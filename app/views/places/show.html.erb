<h1><%= @place.name %></h1>

<p><%= @place.street %></p>
<p><%= @place.zip %> <%= @place.city %></p>

<table>
  <thead>
    <tr>
      <th>overall</th>
      <th>selection</th>
      <th>service</th>
      <th>atmosphere</th>
      <th>food</th>
      <th>reviewcount</th>
      <th>fbscore</th>
      <th>fbcount</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @place.overall %></td>
      <td><%= @place.selection %></td>
      <td><%= @place.service %></td>
      <td><%= @place.atmosphere %></td>
      <td><%= @place.food %></td>
      <td><%= @place.reviewcount %></td>
      <td><%= @place.fbscore %></td>
      <td><%= @place.fbcount %></td>
    </tr>
  </tbody>
</table>

<iframe src="<%= @place.blogmap %>" style="width: 500px; height: 500px;"></iframe>
